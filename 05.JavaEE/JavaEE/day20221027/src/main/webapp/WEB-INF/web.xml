<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>jsjs</servlet-name>
    <servlet-class>HaHa</servlet-class>

<!--    配置初始化参数-->
    <init-param>
      <param-name>setC</param-name>
      <param-value>utf-8</param-value>
    </init-param>
    <load-on-startup>3</load-on-startup>
  </servlet>

  <servlet>
    <servlet-name>addF</servlet-name>
    <servlet-class>FoodServletAdd</servlet-class>
    <init-param>
      <param-name>setC</param-name>
      <param-value>utf-8</param-value>
    </init-param>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>addF</servlet-name>
    <url-pattern>/addFood.do</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>jsjs</servlet-name>
    <url-pattern>/ha.do</url-pattern>
  </servlet-mapping>
</web-app>
